{
  "extends": "astro/tsconfigs/strictest",
  "include": [".astro/types.d.ts", "src/types/**/*", "**/*"],
  "exclude": [
    "dist",
  ],
  "compilerOptions": {
    "verbatimModuleSyntax": true,
    "strictNullChecks": true,
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["src/*"]
    }
  }
}
